{
  "name": "mark",
  "main": "./lib/index",
  "author": "Michael Kleehammer <michael@kleehammer.com>",
  "version": "1.3.3",
  "description": "Provides a buffer mark supporting select-to-mark and goto-mark",
  "activationCommands": {
    "atom-text-editor": [
      "mark:toggle",
      "mark:clear-mark",
      "mark:goto-mark",
      "mark:select-to-mark",
      "mark:select-to-mark-and-copy",
      "mark:select-to-mark-and-cut",
      "mark:swap"
    ]
  },
  "repository": "https://github.com/olmokramer/atom-mark",
  "license": "MIT",
  "engines": {
    "atom": ">0.200.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "eslint": "^1.6.0"
  }
}
